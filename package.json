{
  "name": "meteo",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "liwza",
    "email": "liwza@liwza.com",
    "url": "https://liwza.com"
  },
  "description": "Meteo Group",
  "homepage": "./",
  "main": "public/electron.js",
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.5",
    "@mui/material": "^5.0.6",
    "@mui/styled-engine-sc": "^5.0.4",
    "@mui/styles": "^5.1.0",
    "@reduxjs/toolkit": "^1.6.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "axios": "^0.24.0",
    "bootstrap": "^5.1.3",
    "leaflet": "^1.6.0",
    "node-sass": "^6.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-leaflet": "^3.2.0",
    "react-redux": "^7.2.6",
    "react-router": "^6.0.2",
    "react-router-dom": "^6.0.2",
    "react-scripts": "4.0.3",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.0",
    "serve": "^13.0.2",
    "styled-components": "^5.3.3",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "dev": "concurrently npm:w:*",
    "start": "react-scripts start",
    "w:server": "json-server --w src/assets/db/db.json --port 3003",
    "build": "react-scripts build",
    "w:build": "cross-env BUILD_PATH='./dist/webapp' react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "e:start": "concurrently -k \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3000 && electronmon .\"",
    "e:dev": "concurrently -k \"BROWSER=none npm start\" \"npm:electron\"",
    "electron": "wait-on tcp:3000 && electron .",
    "e:mac": "npm run build && electron-builder -m -c.extraMetadata.main=build/electron.js",
    "e:win": "npm run build && electron-builder -w -c.extraMetadata.main=build/electron.js",
    "e:linux": "npm run build && electron-builder -l -c.extraMetadata.main=build/electron.js"
  },
  "build": {
    "appId": "com.electron.myapp",
    "productName": "My Electron App",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "public",
      "output": "dist/electron"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "deb"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      "last 1 electron version",
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 electron version",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^6.4.0",
    "cross-env": "^7.0.3",
    "dmg-builder": "^22.14.7",
    "electron": "^16.0.0",
    "electron-builder": "^22.13.1",
    "electron-is-dev": "^2.0.0",
    "electronmon": "^2.0.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "redux-devtools": "^3.7.0",
    "redux-devtools-extension": "^2.13.9",
    "wait-on": "^6.0.0"
  }
}

